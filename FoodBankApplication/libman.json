      "destination": "wwwroot/lib/twitter-bootstrap/"
    },
    {
      "library": "font-awesome@6.5.1",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "print-js@1.6.0",
      "destination": "wwwroot/lib/print-js/"
